{
  "name": "messenger-project",
  "version": "1.0.0",
  "engines": {
    "node": "=16.5.0"
  },
  "scripts": {
    "dev": "parcel src/index.html --port 3000 --no-cache",
    "start": "node index.ts",
    "build": "parcel build src/views/login/index.hbs",
    "watch-css": "parcel watch src/common/styles/style.css --dist-dir styles",
    "lint": "eslint ./src/** index.ts",
    "lint-fix": "eslint ./src/** index.ts --fix ",
    "stylelint": "npx stylelint \"**/*.css\" --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@types/node": "17.0.23",
    "parcel": "2.4.0",
    "parcel-plugin-handlebars-precompile": "1.0.2",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-handlebarlabs": "^0.0.6",
    "eslint-plugin-html": "^6.2.0",
    "express": "4.17.3",
    "express-handlebars": "6.0.3",
    "postcss-import": "14.0.2",
    "postcss-nested": "5.0.6",
    "postcss-url": "10.1.3",
    "prettier": "2.5.1",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "nanoid": "^3.3.2"
  }
}
