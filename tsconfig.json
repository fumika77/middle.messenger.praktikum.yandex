{
    "compilerOptions": {
        "skipLibCheck": true,
        "target": "es2017",
        "module": "commonjs",
        "typeRoots": ["node_modules/@types", "typings/app.d.ts"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true
    }
}
