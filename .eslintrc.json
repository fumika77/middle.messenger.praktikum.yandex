{
  "extends": ["airbnb", "handlebarlabs", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "html","prettier"],
  "env": {
    "es6": true
  },
  "rules": {
    "max-len": [2, 100],
    "@typescript-eslint/no-unused-vars": 2
  }
}